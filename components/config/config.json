{
    "ui": {
        "application_font_family": null,
        "application_font_size": 10,
        "node_list_font_family": null,
        "node_list_font_size": 11,
        "node_repr_format": "{node.group} ◎ {node.name}",
        "stylesheet": "ui/juniper.qss",
        "json_highlight": true,
        "json_highlight_colors": {
            "key": "#9cdcfe",
            "value1": "#ce9178",
            "value2": "#b5ad61"
        }
    },
    "qv2ray": {
        "folder": "",
        "config_folder": "",
        "auto_start_qv2ray": false
    },
    "v2ray": {
        "domainMatchers": [
            "linear",
            "mph"
        ],
        "domainMatcher": "mph",
        "balancer_strategies": [
            "random ",
            "leastPing"
        ],
        "balancer_strategy": "leastPing",
        "selector_use_prefixes": true
    },
    "multi_port_forwarding": {
        "default_port_start": 3000,
        "default_port_type": "SOCKS5",
        "inbound_tag_format": "SOCKS5 - {port}",
        "rule_tag_format": "{port} -> {node.group} ◎ {node.name}",
        "outbound_tag_format": "{node.group} ◎ {node.name}",
        "mapping_report_result_path": "results/result-mapping.result.json",
        "mapping_report_template_path": "templates/multi-port-forwarding/result-mapping-as-dict.json",
        "qv2ray_result_path": "results/multi-port-forwarding.result.json",
        "qv2ray_template_path": "templates/multi-port-forwarding/qv2ray-complex-config.json",
        "switchyomega_result_path": "results/OmegaOptions.result.json",
        "switchyomega_proxy_template": "templates/multi-port-forwarding/switchyomega-single-proxy.json"
    },
    "balancer": {
        "outbound_tag_format": "{node.group} ◎ {node.name}",
        "qv2ray_result_path": "results/balancer.result.json",
        "qv2ray_template_path": "templates/balancer/qv2ray-complex-config.json"
    },
    "v2ray_object_templates": {
        "inbound_http": "templates/inbound-http.json",
        "inbound_socks": "templates/inbound-socks.json",
        "outbound_block": "templates/outbound-block.json",
        "outbound_direct": "templates/outbound-direct.json",
        "rules_bypassCN": "templates/rules-bypassCN.json",
        "rules_bypassLAN": "templates/rules-bypassLAN.json"
    }
}